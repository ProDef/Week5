<html>
<head>
	<h1>Your mama's game of Sudoku<h1>
	<title>Sudoku Game</title>
	<link href="/styleshets/application.css" rel="stylesheets" type="text/css">
</head>
<body>
	<div id="sudoku">
		<% 3.times do |row| %>
		<div class='row'>
			<% 3.times do |box| %>
			<div class+'box'>
				<% 3.times do |cell_row| %>
				<div class='cell row'>
					<% 3.times do|cell| %>
					<div class='cell'>
						<input type='text' maxlength='1' autocomplete='off'>
					</div>
				<% end %>
				</div>
			<% end %>
			</div>
		<% end %>
		</div>
	<% end %>
</div>
</body>
</html>


